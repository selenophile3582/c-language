#include <stdio.h>

int main()
{
    int n ,min ,max;
    printf("How many numbers do you want to enter\n");
    scanf("%d", &n);

    int arr[n];
    printf("Enter elements ");
for (int i = 0; i < n; i++) 
{
    scanf("%d",&arr[i]);

}
min = arr[0];
max= arr[0];
for (int i = 0; i < n-1; i++)
{
    if (min>arr[i+1]){
        min=arr[i+1];
    }
    else if (max<arr[i+1])
    max=arr[i+1];
    
}
printf("max is %d \n ",max);
printf("min is %d ",min);
return 0;
}